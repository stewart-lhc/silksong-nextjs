{
  "name": "silk-song-archive-nextjs",
  "version": "1.0.0",
  "description": "Next.js version of the Silk Song Archive - Your ultimate destination for Hollow Knight: Silksong news, updates, and comprehensive game information.",
  "private": true,
  "keywords": [
    "hollow-knight",
    "silksong",
    "team-cherry",
    "metroidvania",
    "indie-game",
    "next.js",
    "react",
    "typescript"
  ],
  "author": "Silk Song Archive Team",
  "license": "MIT",
  "homepage": "https://hollowknightsilksong.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/silk-song-archive"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "npm@10.2.4",
  "scripts": {
    "// ===== DEVELOPMENT =====": "",
    "dev": "next dev --port 3000",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "next dev --turbo",
    
    "// ===== BUILD AND PRODUCTION =====": "",
    "build": "next build",
    "build:analyze": "ENABLE_BUNDLE_ANALYZER=true next build",
    "build:standalone": "next build && next export",
    "build:static": "next build && next export",
    "start": "next start --port 3000",
    "start:prod": "NODE_ENV=production next start",
    
    "// ===== CODE QUALITY =====": "",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --noEmit --incremental --watch",
    
    "// ===== FORMATTING =====": "",
    "format": "prettier --write . --log-level warn",
    "format:check": "prettier --check . --log-level warn",
    "format:staged": "prettier --write --log-level warn",
    
    "// ===== TESTING =====": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test --config=playwright-visual.config.ts",
    "test:all": "npm run type-check && npm run lint:strict && npm run test:coverage && npm run test:e2e",
    
    "// ===== DATABASE =====": "",
    "db:generate": "supabase gen types typescript --project-id gskxekhesnxnghixrfcx > types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:seed": "supabase db seed",
    
    "// ===== PERFORMANCE =====": "",
    "perf:lighthouse": "lhci autorun",
    "perf:bundle": "npm run build:analyze && open .next/analyze/client.html",
    "perf:speed": "next-speed-measure",
    
    "// ===== SECURITY =====": "",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npx audit-ci --moderate",
    
    "// ===== DEPLOYMENT =====": "",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:docker": "docker build -t silk-song-archive .",
    
    "// ===== UTILITIES =====": "",
    "clean": "rimraf .next out dist coverage",
    "clean:all": "rimraf .next out dist coverage node_modules && npm install",
    "clean:deps": "rimraf node_modules package-lock.json && npm install",
    "update": "npx npm-check-updates -u && npm install",
    "update:interactive": "npx npm-check-updates -i",
    
    "// ===== VALIDATION =====": "",
    "validate": "npm run type-check && npm run lint:strict && npm run format:check",
    "validate:ci": "npm run type-check && npm run lint:strict && npm run test:coverage",
    "pre-commit": "npm run validate && npm run test",
    "pre-push": "npm run validate:ci",
    
    "// ===== DOCUMENTATION =====": "",
    "docs:generate": "typedoc --out docs src",
    "docs:serve": "npx serve docs",
    
    "// ===== MAINTENANCE =====": "",
    "maintenance:deps": "npx depcheck && npm outdated",
    "maintenance:size": "npx bundle-size .next/static/**/*.js",
    "maintenance:unused": "npx unimported",
    
    "// ===== POST INSTALL =====": "",
    "postinstall": "husky install"
  },
  "dependencies": {
    "// ===== CORE FRAMEWORK =====": "",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    
    "// ===== STYLING =====": "",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    
    "// ===== UI COMPONENTS =====": "",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.3.0",
    "lucide-react": "^0.445.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.0",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-hover-card": "^1.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    
    "// ===== DATABASE =====": "",
    "@supabase/supabase-js": "^2.45.0",
    
    "// ===== DATA FETCHING =====": "",
    "@tanstack/react-query": "^5.50.0",
    
    "// ===== FORMS =====": "",
    "react-hook-form": "^7.52.0",
    "@hookform/resolvers": "^3.9.0",
    "zod": "^3.23.0",
    
    "// ===== ANIMATIONS =====": "",
    "framer-motion": "^11.3.0",
    
    "// ===== UTILITIES =====": "",
    "date-fns": "^3.6.0",
    "cmdk": "^1.0.0",
    "sonner": "^1.5.0",
    "input-otp": "^1.2.4",
    "embla-carousel-react": "^8.1.8",
    "react-resizable-panels": "^2.0.20",
    "vaul": "^0.9.1",
    "recharts": "^2.12.7",
    "react-day-picker": "^8.10.1",
    
    "// ===== SEO & ANALYTICS =====": "",
    "next-seo": "^6.5.0",
    "next-sitemap": "^4.2.0",
    "@vercel/analytics": "^1.3.0",
    "@vercel/speed-insights": "^1.0.0",
    
    "// ===== THEME =====": "",
    "next-themes": "^0.3.0",
    
    "// ===== PERFORMANCE =====": "",
    "web-vitals": "^4.2.0"
  },
  "devDependencies": {
    "// ===== TYPESCRIPT =====": "",
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    
    "// ===== LINTING =====": "",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "@eslint/eslintrc": "^3.1.0",
    
    "// ===== FORMATTING =====": "",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    
    "// ===== TESTING =====": "",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/user-event": "^14.5.0",
    "playwright": "^1.45.0",
    "@playwright/test": "^1.45.0",
    
    "// ===== BUILD TOOLS =====": "",
    "webpack-bundle-analyzer": "^4.10.0",
    "@svgr/webpack": "^8.1.0",
    "rimraf": "^6.0.0",
    
    "// ===== PERFORMANCE TESTING =====": "",
    "@lhci/cli": "^0.13.0",
    "next-speed-measure": "^0.0.4",
    
    "// ===== CODE QUALITY =====": "",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.0",
    "audit-ci": "^7.0.0",
    "depcheck": "^1.4.7",
    "npm-check-updates": "^17.0.0",
    
    "// ===== DOCUMENTATION =====": "",
    "typedoc": "^0.26.0",
    
    "// ===== UTILITIES =====": "",
    "@next/bundle-analyzer": "^15.1.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run validate:ci"
    }
  }
}